<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="enfuse-gui">
	<schema id="com.gitlab.Latesil.enfuse-gui" path="/com/gitlab/Latesil/enfuse-gui/">
	  <key type="i" name="levels">
      <default>0</default>
      <summary>Levels</summary>
      <description>Number of blending LEVELS to use</description>
    </key>
	  <key type="b" name="auto-levels">
      <default>true</default>
      <summary>Auto levels</summary>
      <description>Auto levels</description>
    </key>
	  <key type="b" name="advanced">
      <default>false</default>
      <summary>Show advanced</summary>
      <description>Show advanced preferences or not</description>
    </key>
	  <key type="s" name="output">
      <default>"a.tif"</default>
      <summary>Output filename</summary>
      <description>Output filename, default is a.tiff</description>
    </key>
	  <key type="s" name="tiff-compression">
      <default>"none"</default>
      <summary>Tiff compression</summary>
      <description>Tiff compression for output file</description>
    </key>
	  <key type="i" name="jpeg-compression">
      <default>0</default>
      <summary>JPEG compression value</summary>
      <description>JPEG compression value</description>
    </key>
	  <key type="i" name="jpeg-compression-arith">
      <default>0</default>
      <summary>JPEG arith compression value</summary>
      <description>JPEG arith compression value</description>
    </key>
	  <key type="b" name="jpeg-compression-boolean">
      <default>true</default>
      <summary>JPEG compression button</summary>
      <description>Set active state of JPEG compression button</description>
    </key>
	  <key type="b" name="jpeg-compression-arith-boolean">
      <default>false</default>
      <summary>JPEG compression arith button</summary>
      <description>Set active state of JPEG arith compression button</description>
    </key>
	</schema>
</schemalist>
